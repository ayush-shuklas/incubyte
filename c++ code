#include<bits/stdc++.h>

using namespace std;

int add(string s)
{
      int k = 0;
   
   int n = s.length();
   vector<int> v;
   int trm = 0;
   vector<string> v1;
   int arr[n];
   for(int i = 0;i<n;i++)
   {
       if(isdigit(s[i]) == true || s[i] == '-')
       {
           if(s[i] == '-')
           {
               trm = 1;
               break;
           }
           else
           {
           int p = (int)(s[i]);
           arr[k] = p - 48;
           k++;
           }
       }
}

int sum = 0;
for(int j = 0;j<k && sum <= 1000;j++)
{
    sum = sum + arr[j];
    
}


if(trm != 1)
return sum;
else
return -1;
}


int main()
{
 string s;
 getline(cin,s);
 int p = add(s);
 if(p>=0)
 cout << p <<endl;
 else
 cout <<"negetives not allowed"<< endl;
}
